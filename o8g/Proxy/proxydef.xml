			<match name="Type" value="None" />
		</matches>
		
		<overlayblocks>
			<link block="blank" />
		</overlayblocks>
		
		<textblocks>
			<link block="name">
				<property name="Name" />
			</link>
			<link block="rules">
				<property name="Rules" />
			</link>
		</textblocks>
	</template>
	
