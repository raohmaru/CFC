	<events>
		<event name="OnTableLoaded"        action="onTableLoaded" />
		<event name="OnGameStarted"        action="onGameStarted" />
		<event name="OnDeckLoaded"         action="onDeckLoaded" />
		<event name="OnCardsMoved"         action="onCardsMoved" />
